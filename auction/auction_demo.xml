	<record model="auction.dates" id="auction_date_1">
		<field name="name">painting Exhibition</field>
		<field name="expo1" eval="time.strftime('%Y')+'-01-01'" />
		<field name="expo2" eval="time.strftime('%Y')+'-02-01'" />

		<field name="auction1" eval="time.strftime('%Y')+'-02-01'" />
		<field name="auction2" eval="time.strftime('%Y')+'-03-01'" />

		<field name="buyer_costs" model="auction_buyer_taxes_rel">1</field>
		<field name="seller_costs" model="aauction_buyer_taxes_rel">1</field>

<!--		<field name="auction_id" ref="auction_deposit_1" />-->
		<field name="buyer_costs" model="account.tax" search="[('name','=','VAT 5.5%')]" />
		<field name="seller_costs" model="account.tax" search="[('name','=','VAT 19.6%')]" />

		<field name="acc_income" model="account.account">1</field>
		<field name="acc_expense" model="account.account">2</field>
		<field name="acc_refund" model="account.account" search="[('code','=','2')]" />
	</record>

	<record model="auction.dates" id="auction_date_2">
