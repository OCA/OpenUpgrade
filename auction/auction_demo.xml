		<field name="expo2" eval="time.strftime('%Y')+'-02-10'" />

		<field name="auction1" eval="time.strftime('%Y')+'-02-12'" />
		<field name="auction2" eval="time.strftime('%Y')+'-03-12'" />

		<field name="seller_costs" model="account.tax" search="[('name','=','VAT 5.5%')]" />
		<field name="buyer_costs" model="account.tax" search="[('name','=','VAT 19.6%')]" />

		<field name="acc_income" model="account.account">10</field>
		<field name="acc_expense" model="account.account">20</field>
		<field name="acc_refund" model="account.account">601</field>
	</record>

