<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
		<record id="comp_input_xml_in" model="etl.component.type">
			<field name="name">input.xml_in</field>
			<field name="code">input.xml_in</field>
		</record>
	</data>

	<data>
		<record id="etl_component_xml_in_form" model="ir.ui.view">
			<field name="name">view.etl.component.xml.in.form</field>
			<field name="model">etl.component</field>
			<field name="arch" type="xml">
				<form string="XML In Component">
					<field name="name" select="1" />
					<field name="type_id"
						domain="[('name','=','input.xml_in')]" />
					<notebook colspan="4">
						<page string="Property">
							<field name="connector_id" select="1" domain="[('type','=','localfile')]"/>
							<field name="transformer_id" />
							<field name="row_limit" />
							<field name="xml_params" />
						</page>
						<page string="Transition Detail">
							<separator string="Incomming Transitions"
								colspan="4" />
							<field name="trans_in_ids" nolabel="1"
								colspan="4" readonly="1" />
							<separator string="Outgoing Transitions"
								colspan="4" />
							<field name="trans_out_ids" nolabel="1"
								colspan="4" readonly="1" />
						</page>
					</notebook>
				</form>

			</field>
		</record>

		<record id="action_etl_component_xml_in_tree"
			model="ir.actions.act_window">
			<field name="name">XML In component</field>
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type_id.name','=','input.xml_in')]</field>
		</record>

		<record id="action_etl_component_xml_in_from"
			model="ir.actions.act_window">
			<field name="res_model">etl.component</field>
			<field name="name">XML In component</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="domain">[('type_id.name','=','input.xml_in')]</field>
			<field name="view_id" ref="etl_component_xml_in_form" />
		</record>

		<record id="action_etl_component_xml_in_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id"
				ref="etl_interface.etl_component_tree" />
			<field name="act_window_id"
				ref="action_etl_component_xml_in_tree" />
		</record>

		<record id="action_etl_component_xml_in_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_component_xml_in_form" />
			<field name="act_window_id"
				ref="action_etl_component_xml_in_tree" />
		</record>

		<menuitem name="XML"
			action="action_etl_component_xml_in_tree"
			parent="etl_interface.menu_etl_component_input_form"
			id="menu_etl_xml_in_tree" />


	</data>
</openerp>