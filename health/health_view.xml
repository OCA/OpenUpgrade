		<field name="type">form</field>
		<field name="arch" type="xml">
		<form string="Patient">
		<field name="name" string="Designation" select="1"/>
			<notebook>
				<page string="General">
					<separator colspan="4" string="General Information"/>
					<field name="nom" select="1" on_change="set_name(nom,prenom)" />
					<field name="nomusage" select="1"/>
					<field name="prenom" on_change="set_name(nom,prenom)" select="1"/>
					<field name="nomreligieux" select="1"/>
					<field domain="[('domain', '=', 'partner')]" name="title"/>
					<field name="ref" select="1"/>
					<field name="lang" />
					<field name="numerosecu" select="1"/>
					<field name="birthdaydate"  select="2" required="True" />
					<field name="admission_date" select="1"  required="True" />
	                <field name="date_sortie" select="1" on_change="desactive(name)"/>
					<newline/>
	                <field name="motif_sortie" select="1"/>
	                <field name="date_liberation" select="1"/>
					<field  name="girage"  select="1"/>
					<field colspan="4" name="room_id"/>
					<field name="congregation" select="1"/>
	                <field name="hospitalisation" select="1" />
	                <field name="active" select="1" />
                </page>
            	<page string = "Adresses">
                 	<field colspan="4" mode="form,tree" name="address" nolabel="1" select="1">
						<form string="Adresses">
							<field name="name" select="1"/>
							<field domain="[('domain', '=', 'contact')]" name="title"/>
							<field name="function"/>
							<field name="type" select="1"/>
							<field name="street" select="1"/>
							<field name="street2"/>
							<newline/>
							<field name="zip" select="1"/>
							<field name="city" select="1"/>
							<newline/>
							<field completion="1" name="country_id" select="1"/>
							<field name="state_id" select="1"/>
							<newline/>
							<field name="phone"/>
							<field name="fax"/>
							<newline/>
							<field name="mobile"/>
							<field name="email"  widget="email"/>
						</form>
					</field>
       		 	</page>
				<page string="details">
					<field name="religion"/>
					<field name="photo" widget="image"/>
					<field name="lieunaissance"/>
					<field name="situation"/>
					<field name="livretremis"/>
					<field name="provenance" />
					<field name="hopitalant" />
					<field name="obseque"/>
					<field name="obsinformations"/>
					<field name="incineration"/>
					<field name="pacemaker"/>
					<field name="doncorps"/>
					<field name="donorganes"/>
					<newline/>
                    <separator colspan="4"/>
					<field name="invalidite"/>
					<newline/>
			        <field  name="invadu" />
			        <field  name="invaau" />
   	                <separator colspan="4"/>
					<field name="respcivil"/>
					<newline/>
					<field  name="respdu" />
		       		<field  name="respau" />
					<newline/>
					<separator colspan="4" string="Insured (e)"/>
			        <field colspan="4" name="assure" />
               		<properties />
					<newline/>
				</page>
				<page string="Wish">
					<field name="medecin" select="1"/>
					<field name="pharmacie" />
					<field name="kine"/>
					<field name="psy" />
					<field name="ergo" />
					<field name="laboratoire" />
					<field name="hopital" />
					<field name="ambulance" />
					<field name="livretremis" />
				</page>
				<page string="Boxes">
					<field  name="regime" />
					<newline/>
					<field  name="caisse" />
					<field  name="ncpaiement" />
					<newline/>
					<field  name="caissedu" />
					<field  name="caisseau" />
					<newline/>
					<field  name="aldtaux" />
                    <field  name="alddu" />
					<field  name="aldau" />
					<newline/>
					<separator colspan="4" string="Mutual"/>
					<field name="mutuelle"/>
					<field name="mutndossier"/>
					<newline/>
					<field  name="mutdu" />
					<field  name="mutau" />
					<newline/>
					<separator colspan="4" string="C.M.U"/>
					<field name="cmu"/>
					<field name="cmundossier"/>
					<newline/>
					<field  name="cmudu" />
					<field  name="cmuau" />
					<separator colspan="4" string="Social Assistance"/>
    				<field name="aidesociale" />
                    <field name="aidesocialedestinataire" />
                    <newline/>
                    <field name="aidesocialemontant" />
                    <field name="aidesocialendossier" />
                    <newline/>
                    <field name="aidesocialedu" />
                    <field name="aidesocialeau" />
                    <separator colspan="4" string="Aide Logement"/>
                    <field name="aidelogement" />
                    <field name="aidelogementdestinataire" />
                    <newline/>
                    <field name="aidelogementmontant" />
                    <field name="aidelogementndossier" />
                    <newline/>
                    <field name="aidelogementdu" />
                    <field name="aidelogementau" />
                    <separator colspan="4" string="APA"/>
                    <field name="apa" />
                    <field name="apadestinataire" />
                    <newline/>
                    <field name="apamontant" />
                    <field name="apandossier" />
                    <newline/>
                    <field name="apadu" />
                    <field name="apaau" />
				</page>
			 	<page string="Absences">
					<field colspan="4" name="absences" nolabel="1"  widget="one2many_list"/>
				</page>
			 	<page string="Prescription Kit">
					<field colspan="4" name="prescriptions" nolabel="1"  widget="one2many_list"/>
				</page>
				<!--<page string="Dossier Medical"/> -->
				<page string="Care Plan">
					<field colspan="4" name="care" nolabel="1" select="1"/>
				</page>
				<page string="Events">
                    <field name="evenements" string=""   widget="one2many_list"/>
				</page>
				<page string="Comment">
					<field name="note" nolabel="1" colspan="4"/>
              	    <field name="category_id"/>
				</page>
			</notebook>
		</form>
		</field>
	</record>
