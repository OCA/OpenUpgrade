	<record model="ir.ui.view" id="view_health_aggir_form">
		<field name="name">health.aggir.form</field>
		<field name="model">health.aggir</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Grid AG GIR">
				<notebook>
					<page string="DISCRIMINANT">
						<field name="name"  select="1"/>
						<newline/>
						<field name="coherence"  required="1"/>
						<field name="orientation" required="1"/>
						<field name="toilette" required="1"/>
						<field name="habillage" required="1"/>
						<field name="alimentation" required="1"/>
						<field name="elimination" required="1"/>
						<field name="transferts" required="1"/>
						<field name="moveint" required="1"/>
						<field name="deplacementexterieur"/>
						<field name="communication"/>
						<field name="gir"  select="1"/>
						<button name="calcul_gir" string="Calcule AG-GIR" colspan="4"  type="object"/>
					</page>
					<page string="DOMESTIC AND SOCIAL DEPENDENCE">
						<field name="gestion"/>
						<field name="cuisine"/>
						<field name="menage"/>
						<field name="transports"/>
						<field name="achats"/>
						<field name="traitement"/>
						<field name="activite"/>
					</page>
				</notebook>
				</form>
			</field>
	</record>
