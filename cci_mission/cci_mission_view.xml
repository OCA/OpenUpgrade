<!-- view for cci_missions_legalization -->
        <record model="ir.ui.view" id="cci_missions_legalization_form">
            <field name="name">cci_missions.legalization.form</field>
            <field name="model">cci_missions.legalization</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Legalization">
                    <notebook>
                    <page string="Legalization Details">
                        <field name="name" select="1" />
                        <field name="date" select="2" />
                        <field name="type_id" select="1" domain="[('section','=','legalization')]"/>
                        <field name="destination_id" select="1"/>
                        <field name="order_partner_id" select="1" on_change="get_legalization_details(order_partner_id)" />
                        <newline/>
                        <field name="asker_name" select="1"/>
                        <field name="sender_name" select="1"/>
                        <field name="partner_member_state"/>
                        <field name="member_price"/>
                        <field name="goods" select="1" />
                        <newline/>
                        <field name="goods_value" />
                        <field name="to_bill" />
                        <field name="quantity_original" select="2"/>
                        <field name="quantity_copies" select="2" />
                        <field name="state" readonly="1"/>
                        <group col="4" colspan="2">
                            <button string="Cancelled by the CCI" name="cancel_cci" states="invoiced" />
                            <button string="Cancelled by the Customer" name="cancel_customer" states="invoiced" />
                        </group>
                        <newline/>
                        <group colspan="2"/>
                        <field name="total" />
                        <newline/>
                        <separator colspan="4" string="Note to Display on the Invoice"/>
                        <field name="text_on_invoice" colspan="4" nolabel="1"/>
                    </page>
                    <page string="Extra Products">
                        <vpaned position="300">
	                        <child1>
	                            <field name="product_ids" nolabel="1" colspan="4" >
	                                <form string="Product Line">
	                                        <field name="name" select="1" colspan="4"/>
	                                        <newline/>
	                                        <field name="product_id" select="1" on_change="product_id_change(product_id)"/>
	                                        <field name="quantity" select="1"/>
	                                        <field name="uos_id"/>
	                                        <field name="price_unit" select="1"/>
	                                </form>
	                            </field>
	                        </child1>
                        	<child2>
                        		<label string="" colspan="2"/><field name="sub_total" coslapn="2"/>
                        	</child2>
                        </vpaned>
                    </page>
                    <page string="Links">
                        <field name="embassy_folder_id" />
                        <newline/>
                        <field name="certificate_id" />
                        <newline/>
                        <field name="invoice_id" />
                    </page>
                </notebook>
                </form>
            </field>
        </record>

