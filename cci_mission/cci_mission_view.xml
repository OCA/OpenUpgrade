                    <newline/>
                    <field name="embassy_folder_id" />
                    <newline/>
                    <field name="state" select="2"/>
                    <newline/>
                    <separator colspan="4" string="Note to Display on the Invoice"/>
                    <field name="text_on_invoice" colspan="4" nolabel="1"/>
                </form>
            </field>
        </record>

