            <field name="type">graph</field>
            <field name="arch" type="xml">
                <graph string="Bugs by State" type="pie">
                    <field name="state"/>
                    <field name="nbr" operator="+"/>
                </graph>
            </field>
        </record>
        <record model="ir.ui.view" id="view_crm_case_bugs_user_tree">
